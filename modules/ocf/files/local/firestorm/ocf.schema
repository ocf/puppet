#OCF additional attributes

attributetype ( 1.3.6.1.4.1.7153.1.2.1 NAME 'calNetuid'
        DESC 'An integer uniquely identifying a user in the CalNet Directory'
        EQUALITY integerMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.7153.1.3.1 NAME 'oslgid'
        DESC 'An integer uniquely identifying a OSL Student Group'
        EQUALITY integerMatch
        SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 SINGLE-VALUE )

objectclass ( 1.3.6.1.4.1.7153.1.1.1
  NAME 'ocfAccount'
  DESC 'Attributes for OCF Accounts'
  AUXILIARY
  MAY (calNetuid $ oslgid))
