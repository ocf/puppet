[global]
directory : /var/spool/cups/
# In development mode, store ALL print job data in the print spool dir forever.
# This takes up space very quickly, so, if you are going to test the dev server
# on the whole lab, delete old files every few hours via a cron job or simlar.
<% if not @dev_config %>
keepfiles : no
<% else %>
keepfiles : yes
<% end %>
prehook_enforcer : sudo -Eu ocfenforcer /usr/local/bin/enforcer prehook
posthook_enforcer : sudo -Eu ocfenforcer /usr/local/bin/enforcer posthook
