# Login access control table, scanned for first entry matching user@origin
# format: permission : users/groups : origins
#
+ : ALL : cron crond
- : ALL : tsunami.ocf.berkeley.edu
+ : root : LOCAL hal.ocf.berkeley.edu pandemic.ocf.berkeley.edu jaws.ocf.berkeley.edu supernova.ocf.berkeley.edu
+ : apt-dater : lightning.ocf.berkeley.edu
+ : dumper : pollution.ocf.berkeley.edu
+ : nessus : .security.berkeley.edu
<% @ulogin.each do |user,from| -%>
<% if not user.nil? and not from.nil? -%>
+ : <%= user %> : <%= from %>
<% end -%>
<% end -%>
+ : (ocfroot) : ALL
+ : (ocfstaff) : ALL
+ : (sys) : ALL
<% @glogin.each do |val| -%>
+ : (<%= val %>) : ALL
<% end -%>
- : ALL : ALL
